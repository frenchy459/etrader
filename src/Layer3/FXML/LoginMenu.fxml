          <Font name="Roboto Medium" size="20.0"/>
        </font>
    </JFXButton>
    <JFXTextField fx:id="userName" focusColor="#039bf5" layoutX="72.0" layoutY="278.0" prefHeight="31.0"
                  prefWidth="456.0" promptText="USERNAME" stylesheets="@../CSS/Stylesheet.css">
